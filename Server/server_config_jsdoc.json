{
  "plugins": ["plugins/markdown"],
  "markdown":
  {
    "hardwrap": true
  },
  "recurseDepth": 10,
  "source":
  {
    "include": ["./Server/db.js", "./Server/getMaxID.js", "./Server/UserData.js"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags":
  {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates":
  {
    "cleverLinks": false,
    "monospaceLinks": false
  }

}