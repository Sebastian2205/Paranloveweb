         * @param {string} plz
         * @param {string} auswertung
         */
        function addMatch(tabelle, nutzername, geschlecht, geburtstag, plz, auswertung) {
            const zeile = document.getElementById(tabelle).insertRow(1);
            let spalte1 = zeile.insertCell(0);
            let spalte2 = zeile.insertCell(1);
            let spalte3 = zeile.insertCell(2);
            let spalte4 = zeile.insertCell(3);
            spalte1.innerHTML = nutzername;
            spalte2.innerHTML = geschlecht;
            spalte3.innerHTML = geburtstag;
            spalte4.innerHTML = plz;

            if (auswertung === "gelb") {
                zeile.style.backgroundColor = "yellow";
            }
            else {
                zeile.style.backgroundColor = "green";
                zeile.style.color = "white";
            }
    }
