         * @description Funktion zum Anpassen der Anzeige (in der Desktop-Ansicht)
         * @param {string} id_html ID eines der HTML-Elemente, dass zur Steuerung der Anicht benutzt wird.
         */
        function toogleVisualNavigator_Desktop(id_html) {
            // Reset Anzeige der Tabs
            document.getElementById("nav_home").style.backgroundColor = "#e70032";
            document.getElementById("nav_profil").style.backgroundColor = "#e70032";
            document.getElementById("nav_nachrichten").style.backgroundColor = "#e70032";
            document.getElementById("nav_logout").style.backgroundColor = "#e70032";

            // Markiere den ausgewählten (aktiven) Tab
            document.getElementById(id_html).style.backgroundColor = "white";

            // Reset Anzeige der Displayinhalte
            document.getElementById("home").hidden = true;
            document.getElementById("nutzerprofil").hidden = true;
            document.getElementById("nachrichtensystem").hidden = true;
            document.getElementById("logout").hidden = true;

            // Decke den aktiven Displayinhalt auf
            switch (id_html) {
                case "nav_home":
                    document.getElementById("home").hidden = false;
                    break;
                case "nav_profil":
                    document.getElementById("nutzerprofil").hidden = false;
                    break;
                case "nav_nachrichten":
                    document.getElementById("nachrichtensystem").hidden = false;
                    break;
                case "nav_logout":
                    document.getElementById("logout").hidden = true;
                    //Logout Funktion
                    break;
            }
        }

